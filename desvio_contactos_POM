-- listado de campañas POM https://docs.google.com/spreadsheets/d/1iq7D9_d8YCnrO4G-2zrybB6O1lY8pzIwunKht-24Axk/edit#gid=0

DECLARE FECHA_DESDE DATE DEFAULT '2022-07-01'
;
DECLARE FECHA_HASTA DATE DEFAULT '2023-03-31'
;


SELECT
    DATE_TRUNC(A.fecha, MONTH) AS Mes
  , A.site
  , A.businessUnit

  -- INICIO columnas si coincide con filtros de seguimiento diario POM
  , (CASE WHEN A.vertical = 'MOTORS' THEN 'Coincide'
          WHEN A.vertical = 'REAL_ESTATE' THEN 'Coincide' ELSE 'No coincide' END) AS coincide_vertical
  , (CASE WHEN upper(trafficSource.medium) in ('DISPLAY','CPC','DPA','SOCIAL','MEDIA','EMAILS','EMAIL','SOCIAL','SOCIALMEDIA','RETARGETING') THEN 'Coincide' ELSE 'No coincide' END) AS coincide_medium
  -- FIN columnas si coincide con filtros de seguimiento diario POM

  , (CASE WHEN UPPER(A.vertical) LIKE ('REA%') THEN 'RE'
          WHEN UPPER(A.vertical) LIKE ('MO%') THEN  'MO'
          WHEN UPPER(A.vertical) LIKE ('SER%') THEN 'SE' END) AS vertical_que_recibio_lead

  , (CASE WHEN UPPER(A.vertical) LIKE ('SER%') THEN '-'
          WHEN COALESCE(B.Categoria_l4,C.Categoria_l4) IN ('Desarrollos Inmobiliarios','Emprendimientos','Lançamentos','Proyectos') THEN 'Desarrollos'
          WHEN COALESCE(B.Categoria_l3,C.Categoria_l3) IN ('Arriendo Temporal','Temporada','Alquiler Temporal','Alquiler Temporada','Alquiler Temporario','Alquiler Vacacional','Renta Vacacional') THEN 'Alquiler temporal'
          WHEN COALESCE(B.Categoria_l3,C.Categoria_l3) IN ('Venta','Venda') THEN 'Venta'
          WHEN COALESCE(B.Categoria_l3,C.Categoria_l3) IN ('Alquiler','Traspaso','Aluguel','Renta','Arriendo') THEN 'Alquiler' 
          ELSE COALESCE(B.Categoria_l3,C.Categoria_l3) END) AS operacion_que_recibio_lead

    -- clasicacion en base a glosario de campañas POM
  , (CASE WHEN UPPER(trafficSource.campaign) = 'MLC_CLASSI_FB_AO_RE_X_DABA_NEWB_LEAD_MERCADOLIBRE' AND UPPER(trafficSource.adContent) = 'ALQUILER' THEN 'RE - ALQUILER'
          WHEN UPPER(trafficSource.campaign) = 'MLC_CLASSI_FB_AO_RE_X_DABA_NEWB_LEAD_MERCADOLIBRE' AND UPPER(trafficSource.adContent) = 'VENTA' THEN 'RE - VENTA'
          WHEN UPPER(trafficSource.campaign) = 'MLC_CLASSI_FB_AO_RE_X_DARE_REPB_LEAD_MERCADOLIBRE' AND UPPER(trafficSource.adContent) = 'ALQUILER' THEN 'RE - ALQUILER'
          WHEN UPPER(trafficSource.campaign) = 'MLC_CLASSI_FB_AO_RE_X_DARE_REPB_LEAD_MERCADOLIBRE' AND UPPER(trafficSource.adContent) = 'VENTA' THEN 'RE - VENTA'
          WHEN UPPER(trafficSource.campaign) = 'MLC_CLASSI_FB_AO_RE_X_DARE_REPB_LEAD_MERCADOLIBRE' THEN 'RE'

          WHEN UPPER(trafficSource.campaign) = 'MLB_CLASSI_FB_AO_MOT_X_AIA_ALLB_LEAD_AUTOS-Y-CAMIONETAS2FB' THEN 'MO - DEALERS' 
