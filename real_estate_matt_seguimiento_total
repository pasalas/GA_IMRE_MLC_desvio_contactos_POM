              SUM(IF(
                (ITE_QUOT_SOURCE IN ('facebook') OR QUESTION_APP_ID IN (6606988212253970,6557539776310564) ) -- FB FORMS
            OR (is_POM_VIS = 'POM VIS'  
              AND POM_VIS_subgroup IN ('FACEBOOK FORMS') )
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
              , MATT_PORC_TIMEDECAY
              , NULL) )
            , 100) AS pom_vis_facebook_forms_alq_tmp_vivienda_timedecay_contact_attribution

          -- usuarios que contactaron
          , COUNT(
              DISTINCT
                IF(
                (ITE_QUOT_SOURCE IN ('facebook') OR QUESTION_APP_ID IN (6606988212253970,6557539776310564) ) -- FB FORMS
            OR (is_POM_VIS = 'POM VIS'  
              AND POM_VIS_subgroup IN ('FACEBOOK FORMS') )
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
                , COALESCE(CAST(A.CUS_CUST_ID_BUY AS STRING), B.SESSION_ID)
                , NULL) ) AS pom_vis_facebook_forms_alq_tmp_vivienda_user_contact

          -- usuarios logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                (ITE_QUOT_SOURCE IN ('facebook') OR QUESTION_APP_ID IN (6606988212253970,6557539776310564) ) -- FB FORMS
            OR (is_POM_VIS = 'POM VIS'  
              AND POM_VIS_subgroup IN ('FACEBOOK FORMS') )
              AND A.CUS_CUST_ID_BUY IS NOT NULL
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
