        , SAFE_DIVIDE(pom_vis_rtb_vivienda_user_not_logg_contact, ( COALESCE(pom_vis_rtb_vivienda_user_logg_contact, 0) + COALESCE(pom_vis_rtb_vivienda_user_not_logg_contact,0) ) ) AS perc_pom_vis_rtb_vivienda_user_not_logg_contact
        , pom_vis_rtb_vivienda_item_contacted
