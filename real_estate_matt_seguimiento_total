      ------------------------
      -- TOTAL (sin apertura de canal)
      ------------------------
        -- leads
        , total_alquiler_vivienda_contact
        , SAFE_DIVIDE(pom_vis_alquiler_vivienda_timedecay_contact_attribution, total_alquiler_vivienda_contact) AS perc_pom_vis_alquiler_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(direct_alquiler_vivienda_timedecay_contact_attribution, total_alquiler_vivienda_contact) AS perc_direct_alquiler_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(seo_alquiler_vivienda_timedecay_contact_attribution, total_alquiler_vivienda_contact) AS perc_seo_alquiler_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(own_channels_alquiler_vivienda_timedecay_contact_attribution, total_alquiler_vivienda_contact) AS perc_own_channels_alquiler_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(matt_other_alquiler_vivienda_timedecay_contact_attribution, total_alquiler_vivienda_contact) AS perc_matt_other_alquiler_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_desvio_alquiler_vivienda_timedecay_contact_attribution, total_alquiler_vivienda_contact) AS perc_pom_vis_desvio_alquiler_vivienda_timedecay_contact_attribution

        , total_alquiler_vivienda_user_contact
        , SAFE_DIVIDE(pom_vis_alquiler_vivienda_user_contact ,(COALESCE(pom_vis_alquiler_vivienda_user_contact, 0) + COALESCE(direct_alquiler_vivienda_user_contact, 0) + COALESCE(seo_alquiler_vivienda_user_contact, 0) + COALESCE(own_channels_alquiler_vivienda_user_contact, 0) + COALESCE(matt_other_alquiler_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alquiler_vivienda_user_contact, 0) ) ) perc_pom_vis_alquiler_vivienda_user_contact

        , SAFE_DIVIDE(direct_alquiler_vivienda_user_contact ,(COALESCE(pom_vis_alquiler_vivienda_user_contact, 0) + COALESCE(direct_alquiler_vivienda_user_contact, 0) + COALESCE(seo_alquiler_vivienda_user_contact, 0) + COALESCE(own_channels_alquiler_vivienda_user_contact, 0) + COALESCE(matt_other_alquiler_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alquiler_vivienda_user_contact, 0) ) ) perc_direct_alquiler_vivienda_user_contact

        , SAFE_DIVIDE(seo_alquiler_vivienda_user_contact ,(COALESCE(pom_vis_alquiler_vivienda_user_contact, 0) + COALESCE(direct_alquiler_vivienda_user_contact, 0) + COALESCE(seo_alquiler_vivienda_user_contact, 0) + COALESCE(own_channels_alquiler_vivienda_user_contact, 0) + COALESCE(matt_other_alquiler_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alquiler_vivienda_user_contact, 0) ) ) perc_seo_alquiler_vivienda_user_contact

        , SAFE_DIVIDE(own_channels_alquiler_vivienda_user_contact ,(COALESCE(pom_vis_alquiler_vivienda_user_contact, 0) + COALESCE(direct_alquiler_vivienda_user_contact, 0) + COALESCE(seo_alquiler_vivienda_user_contact, 0) + COALESCE(own_channels_alquiler_vivienda_user_contact, 0) + COALESCE(matt_other_alquiler_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alquiler_vivienda_user_contact, 0) ) ) perc_own_channels_alquiler_vivienda_user_contact

        , SAFE_DIVIDE(matt_other_alquiler_vivienda_user_contact ,(COALESCE(pom_vis_alquiler_vivienda_user_contact, 0) + COALESCE(direct_alquiler_vivienda_user_contact, 0) + COALESCE(seo_alquiler_vivienda_user_contact, 0) + COALESCE(own_channels_alquiler_vivienda_user_contact, 0) + COALESCE(matt_other_alquiler_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alquiler_vivienda_user_contact, 0) ) ) perc_matt_other_alquiler_vivienda_user_contact

        , SAFE_DIVIDE(pom_vis_desvio_alquiler_vivienda_user_contact ,(COALESCE(pom_vis_alquiler_vivienda_user_contact, 0) + COALESCE(direct_alquiler_vivienda_user_contact, 0) + COALESCE(seo_alquiler_vivienda_user_contact, 0) + COALESCE(own_channels_alquiler_vivienda_user_contact, 0) + COALESCE(matt_other_alquiler_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alquiler_vivienda_user_contact, 0) ) ) perc_pom_vis_desvio_alquiler_vivienda_user_contact



        , SAFE_DIVIDE(total_alquiler_vivienda_contact, total_alquiler_vivienda_user_contact) AS avg_alquiler_vivienda_contact_per_user
        , total_alquiler_vivienda_user_logg_contact
        , total_alquiler_vivienda_user_not_logg_contact
        , SAFE_DIVIDE(total_alquiler_vivienda_user_logg_contact, ( COALESCE(total_alquiler_vivienda_user_logg_contact, 0) + COALESCE(total_alquiler_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_alquiler_vivienda_user_logg_contact
        , SAFE_DIVIDE(total_alquiler_vivienda_user_not_logg_contact, ( COALESCE(total_alquiler_vivienda_user_logg_contact, 0) + COALESCE(total_alquiler_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_alquiler_vivienda_user_not_logg_contact
        , total_alquiler_vivienda_item_contacted
        , SAFE_DIVIDE(total_alquiler_vivienda_contact, total_alquiler_vivienda_item_contacted) AS avg_total_alquiler_vivienda_item_contacted
        , total_alquiler_vivienda_seller_contacted
        , SAFE_DIVIDE(total_alquiler_vivienda_contact, total_alquiler_vivienda_seller_contacted) AS avg_total_alquiler_vivienda_seller_contacted


