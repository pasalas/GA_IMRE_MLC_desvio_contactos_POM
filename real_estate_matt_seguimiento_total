    ------------------------------------------------
    ---- Vivienda: casas, dptos, phs y townhouses
    ------------------------------------------------

      ------------------------
      -- TOTAL (sin apertura de canal)
      ------------------------
        -- leads
        , total_vivienda_contact
        , SAFE_DIVIDE(pom_vis_vivienda_timedecay_contact_attribution, total_vivienda_contact) AS perc_pom_vis_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(direct_vivienda_timedecay_contact_attribution, total_vivienda_contact) AS perc_direct_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(seo_vivienda_timedecay_contact_attribution, total_vivienda_contact) AS perc_seo_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(own_channels_vivienda_timedecay_contact_attribution, total_vivienda_contact) AS perc_own_channels_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(matt_other_vivienda_timedecay_contact_attribution, total_vivienda_contact) AS perc_matt_other_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_desvio_vivienda_timedecay_contact_attribution, total_vivienda_contact) AS perc_pom_vis_desvio_vivienda_timedecay_contact_attribution

        , total_vivienda_user_contact
        , SAFE_DIVIDE(pom_vis_vivienda_user_contact ,(COALESCE(pom_vis_vivienda_user_contact, 0) + COALESCE(direct_vivienda_user_contact, 0) + COALESCE(seo_vivienda_user_contact, 0) + COALESCE(own_channels_vivienda_user_contact, 0) + COALESCE(matt_other_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_vivienda_user_contact, 0) ) ) perc_pom_vis_vivienda_user_contact

        , SAFE_DIVIDE(direct_vivienda_user_contact ,(COALESCE(pom_vis_vivienda_user_contact, 0) + COALESCE(direct_vivienda_user_contact, 0) + COALESCE(seo_vivienda_user_contact, 0) + COALESCE(own_channels_vivienda_user_contact, 0) + COALESCE(matt_other_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_vivienda_user_contact, 0) ) ) perc_direct_vivienda_user_contact

        , SAFE_DIVIDE(seo_vivienda_user_contact ,(COALESCE(pom_vis_vivienda_user_contact, 0) + COALESCE(direct_vivienda_user_contact, 0) + COALESCE(seo_vivienda_user_contact, 0) + COALESCE(own_channels_vivienda_user_contact, 0) + COALESCE(matt_other_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_vivienda_user_contact, 0) ) ) perc_seo_vivienda_user_contact

        , SAFE_DIVIDE(own_channels_vivienda_user_contact ,(COALESCE(pom_vis_vivienda_user_contact, 0) + COALESCE(direct_vivienda_user_contact, 0) + COALESCE(seo_vivienda_user_contact, 0) + COALESCE(own_channels_vivienda_user_contact, 0) + COALESCE(matt_other_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_vivienda_user_contact, 0) ) ) perc_own_channels_vivienda_user_contact

        , SAFE_DIVIDE(matt_other_vivienda_user_contact ,(COALESCE(pom_vis_vivienda_user_contact, 0) + COALESCE(direct_vivienda_user_contact, 0) + COALESCE(seo_vivienda_user_contact, 0) + COALESCE(own_channels_vivienda_user_contact, 0) + COALESCE(matt_other_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_vivienda_user_contact, 0) ) ) perc_matt_other_vivienda_user_contact

        , SAFE_DIVIDE(pom_vis_desvio_vivienda_user_contact ,(COALESCE(pom_vis_vivienda_user_contact, 0) + COALESCE(direct_vivienda_user_contact, 0) + COALESCE(seo_vivienda_user_contact, 0) + COALESCE(own_channels_vivienda_user_contact, 0) + COALESCE(matt_other_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_vivienda_user_contact, 0) ) ) perc_pom_vis_desvio_vivienda_user_contact



        , SAFE_DIVIDE(total_vivienda_contact, total_vivienda_user_contact) AS avg_vivienda_contact_per_user
        , total_vivienda_user_logg_contact
        , total_vivienda_user_not_logg_contact
        , SAFE_DIVIDE(total_vivienda_user_logg_contact, ( COALESCE(total_vivienda_user_logg_contact, 0) + COALESCE(total_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_vivienda_user_logg_contact
        , SAFE_DIVIDE(total_vivienda_user_not_logg_contact, ( COALESCE(total_vivienda_user_logg_contact, 0) + COALESCE(total_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_vivienda_user_not_logg_contact
        , total_vivienda_item_contacted
        , SAFE_DIVIDE(total_vivienda_contact, total_vivienda_item_contacted) AS avg_total_vivienda_item_contacted
        , total_vivienda_seller_contacted
        , SAFE_DIVIDE(total_vivienda_contact, total_vivienda_seller_contacted) AS avg_total_vivienda_seller_contacted


