        , SAFE_DIVIDE(pom_vis_brandformance_alquiler_vivienda_timedecay_contact_attribution, pom_vis_brandformance_alquiler_vivienda_seller_contacted) AS avg_pom_vis_brandformance_alquiler_vivienda_seller_contacted


        -- cost clicks impressions
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_alquiler_vivienda_timedecay_contact_attribution) AS pom_vis_brandformance_alquiler_vivienda_lc_cost_per_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_alquiler_vivienda_user_contact) AS pom_vis_brandformance_alquiler_vivienda_lc_cost_per_user_contact
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_alquiler_vivienda_item_contacted) AS pom_vis_brandformance_alquiler_vivienda_lc_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_alquiler_vivienda_seller_contacted) AS pom_vis_brandformance_alquiler_vivienda_lc_cost_per_seller_contacted

        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alquiler_vivienda_timedecay_contact_attribution) AS pom_vis_brandformance_alquiler_vivienda_usd_cost_per_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alquiler_vivienda_user_contact) AS pom_vis_brandformance_alquiler_vivienda_usd_cost_per_user_contact
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alquiler_vivienda_item_contacted) AS pom_vis_brandformance_alquiler_vivienda_usd_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alquiler_vivienda_seller_contacted) AS pom_vis_brandformance_alquiler_vivienda_usd_cost_per_seller_contacted


