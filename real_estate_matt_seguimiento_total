        , SAFE_DIVIDE(pom_vis_brandformance_venta_vivienda_timedecay_contact_attribution, pom_vis_brandformance_venta_vivienda_user_contact) AS avg_pom_vis_brandformance_venta_vivienda_contact_per_user
        , pom_vis_brandformance_venta_vivienda_user_logg_contact
        , pom_vis_brandformance_venta_vivienda_user_not_logg_contact
        , SAFE_DIVIDE(pom_vis_brandformance_venta_vivienda_user_logg_contact, ( COALESCE(pom_vis_brandformance_venta_vivienda_user_logg_contact, 0) + COALESCE(pom_vis_brandformance_venta_vivienda_user_not_logg_contact,0) ) ) AS perc_pom_vis_brandformance_venta_vivienda_user_logg_contact
        , SAFE_DIVIDE(pom_vis_brandformance_venta_vivienda_user_not_logg_contact, ( COALESCE(pom_vis_brandformance_venta_vivienda_user_logg_contact, 0) + COALESCE(pom_vis_brandformance_venta_vivienda_user_not_logg_contact,0) ) ) AS perc_pom_vis_brandformance_venta_vivienda_user_not_logg_contact
        , pom_vis_brandformance_venta_vivienda_item_contacted
