      ------------------------
      -- TOTAL (sin apertura de canal)
      ------------------------
        -- leads
        , total_alq_tmp_vivienda_contact
        , SAFE_DIVIDE(pom_vis_alq_tmp_vivienda_timedecay_contact_attribution, total_alq_tmp_vivienda_contact) AS perc_pom_vis_alq_tmp_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(direct_alq_tmp_vivienda_timedecay_contact_attribution, total_alq_tmp_vivienda_contact) AS perc_direct_alq_tmp_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(seo_alq_tmp_vivienda_timedecay_contact_attribution, total_alq_tmp_vivienda_contact) AS perc_seo_alq_tmp_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(own_channels_alq_tmp_vivienda_timedecay_contact_attribution, total_alq_tmp_vivienda_contact) AS perc_own_channels_alq_tmp_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(matt_other_alq_tmp_vivienda_timedecay_contact_attribution, total_alq_tmp_vivienda_contact) AS perc_matt_other_alq_tmp_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_desvio_alq_tmp_vivienda_timedecay_contact_attribution, total_alq_tmp_vivienda_contact) AS perc_pom_vis_desvio_alq_tmp_vivienda_timedecay_contact_attribution

        , total_alq_tmp_vivienda_user_contact
        , SAFE_DIVIDE(pom_vis_alq_tmp_vivienda_user_contact ,(COALESCE(pom_vis_alq_tmp_vivienda_user_contact, 0) + COALESCE(direct_alq_tmp_vivienda_user_contact, 0) + COALESCE(seo_alq_tmp_vivienda_user_contact, 0) + COALESCE(own_channels_alq_tmp_vivienda_user_contact, 0) + COALESCE(matt_other_alq_tmp_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alq_tmp_vivienda_user_contact, 0) ) ) perc_pom_vis_alq_tmp_vivienda_user_contact

        , SAFE_DIVIDE(direct_alq_tmp_vivienda_user_contact ,(COALESCE(pom_vis_alq_tmp_vivienda_user_contact, 0) + COALESCE(direct_alq_tmp_vivienda_user_contact, 0) + COALESCE(seo_alq_tmp_vivienda_user_contact, 0) + COALESCE(own_channels_alq_tmp_vivienda_user_contact, 0) + COALESCE(matt_other_alq_tmp_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alq_tmp_vivienda_user_contact, 0) ) ) perc_direct_alq_tmp_vivienda_user_contact

        , SAFE_DIVIDE(seo_alq_tmp_vivienda_user_contact ,(COALESCE(pom_vis_alq_tmp_vivienda_user_contact, 0) + COALESCE(direct_alq_tmp_vivienda_user_contact, 0) + COALESCE(seo_alq_tmp_vivienda_user_contact, 0) + COALESCE(own_channels_alq_tmp_vivienda_user_contact, 0) + COALESCE(matt_other_alq_tmp_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alq_tmp_vivienda_user_contact, 0) ) ) perc_seo_alq_tmp_vivienda_user_contact

        , SAFE_DIVIDE(own_channels_alq_tmp_vivienda_user_contact ,(COALESCE(pom_vis_alq_tmp_vivienda_user_contact, 0) + COALESCE(direct_alq_tmp_vivienda_user_contact, 0) + COALESCE(seo_alq_tmp_vivienda_user_contact, 0) + COALESCE(own_channels_alq_tmp_vivienda_user_contact, 0) + COALESCE(matt_other_alq_tmp_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alq_tmp_vivienda_user_contact, 0) ) ) perc_own_channels_alq_tmp_vivienda_user_contact

        , SAFE_DIVIDE(matt_other_alq_tmp_vivienda_user_contact ,(COALESCE(pom_vis_alq_tmp_vivienda_user_contact, 0) + COALESCE(direct_alq_tmp_vivienda_user_contact, 0) + COALESCE(seo_alq_tmp_vivienda_user_contact, 0) + COALESCE(own_channels_alq_tmp_vivienda_user_contact, 0) + COALESCE(matt_other_alq_tmp_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alq_tmp_vivienda_user_contact, 0) ) ) perc_matt_other_alq_tmp_vivienda_user_contact

        , SAFE_DIVIDE(pom_vis_desvio_alq_tmp_vivienda_user_contact ,(COALESCE(pom_vis_alq_tmp_vivienda_user_contact, 0) + COALESCE(direct_alq_tmp_vivienda_user_contact, 0) + COALESCE(seo_alq_tmp_vivienda_user_contact, 0) + COALESCE(own_channels_alq_tmp_vivienda_user_contact, 0) + COALESCE(matt_other_alq_tmp_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_alq_tmp_vivienda_user_contact, 0) ) ) perc_pom_vis_desvio_alq_tmp_vivienda_user_contact



        , SAFE_DIVIDE(total_alq_tmp_vivienda_contact, total_alq_tmp_vivienda_user_contact) AS avg_alq_tmp_vivienda_contact_per_user
        , total_alq_tmp_vivienda_user_logg_contact
        , total_alq_tmp_vivienda_user_not_logg_contact
        , SAFE_DIVIDE(total_alq_tmp_vivienda_user_logg_contact, ( COALESCE(total_alq_tmp_vivienda_user_logg_contact, 0) + COALESCE(total_alq_tmp_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_alq_tmp_vivienda_user_logg_contact
        , SAFE_DIVIDE(total_alq_tmp_vivienda_user_not_logg_contact, ( COALESCE(total_alq_tmp_vivienda_user_logg_contact, 0) + COALESCE(total_alq_tmp_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_alq_tmp_vivienda_user_not_logg_contact
        , total_alq_tmp_vivienda_item_contacted
        , SAFE_DIVIDE(total_alq_tmp_vivienda_contact, total_alq_tmp_vivienda_item_contacted) AS avg_total_alq_tmp_vivienda_item_contacted
        , total_alq_tmp_vivienda_seller_contacted
        , SAFE_DIVIDE(total_alq_tmp_vivienda_contact, total_alq_tmp_vivienda_seller_contacted) AS avg_total_alq_tmp_vivienda_seller_contacted


