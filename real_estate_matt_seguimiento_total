                , COALESCE(CAST(A.CUS_CUST_ID_BUY AS STRING), B.SESSION_ID)
                , NULL) ) AS pom_vis_vivienda_user_contact

          -- usuarios logueados que contactaron
          , COUNT(
              DISTINCT
