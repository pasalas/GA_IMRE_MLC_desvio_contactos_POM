              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
                , B.SESSION_ID
                , NULL) ) AS pom_vis_facebook_alq_tmp_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
                , lead_item_id
                , NULL) ) AS pom_vis_facebook_alq_tmp_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
                , B.cus_cust_id_sel
                , NULL) ) AS pom_vis_facebook_alq_tmp_vivienda_seller_contacted

        ------------------------------------------------
