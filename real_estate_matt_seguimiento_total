                IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND A.CUS_CUST_ID_BUY IS NULL
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , B.SESSION_ID
                , NULL) ) AS pom_vis_brandformance_alquiler_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , lead_item_id
                , NULL) ) AS pom_vis_brandformance_alquiler_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , B.cus_cust_id_sel
                , NULL) ) AS pom_vis_brandformance_alquiler_vivienda_seller_contacted
      ------------------------------------------------
      ---- Alquiler temporal vivienda
      ------------------------------------------------
        ------------------------------------------------
        ---- TOTAL (sin apertura de canal)
        ------------------------------------------------
          -- contact timedecay
          , COUNT(DISTINCT IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')  , LEAD_ID, NULL) ) AS total_alq_tmp_vivienda_contact

          -- usuarios que contactaron
          , COUNT(
              DISTINCT
                IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
              , COALESCE(CAST(A.CUS_CUST_ID_BUY AS STRING), B.SESSION_ID)
              , NULL) 
                ) AS total_alq_tmp_vivienda_user_contact

          -- usuarios logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                  A.CUS_CUST_ID_BUY IS NOT NULL
                AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
                , A.CUS_CUST_ID_BUY
                , NULL) ) AS total_alq_tmp_vivienda_user_logg_contact

          -- usuarios no logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                  A.CUS_CUST_ID_BUY IS NULL
                AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional')
                , B.SESSION_ID
                , NULL) ) AS total_alq_tmp_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                  IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional'), lead_item_id, NULL)
              ) AS total_alq_tmp_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler temporal','Alquiler temporada','Arriendo temporal','Temporada','Renta vacacional','Alquiler vacacional'), B.cus_cust_id_sel, NULL)
