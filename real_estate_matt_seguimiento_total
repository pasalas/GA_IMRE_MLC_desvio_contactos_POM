              DISTINCT
                IF(
                (ITE_QUOT_SOURCE IN ('facebook') OR QUESTION_APP_ID IN (6606988212253970,6557539776310564) ) -- FB FORMS
            OR (is_POM_VIS = 'POM VIS'  
              AND POM_VIS_subgroup IN ('FACEBOOK FORMS') )
              AND A.CUS_CUST_ID_BUY IS NOT NULL
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , A.CUS_CUST_ID_BUY
                , NULL) ) AS pom_vis_facebook_forms_venta_vivienda_user_logg_contact

          -- usuarios no logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                (ITE_QUOT_SOURCE IN ('facebook') OR QUESTION_APP_ID IN (6606988212253970,6557539776310564) ) -- FB FORMS
            OR (is_POM_VIS = 'POM VIS'  
              AND POM_VIS_subgroup IN ('FACEBOOK FORMS') )
              AND A.CUS_CUST_ID_BUY IS NULL
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , B.SESSION_ID
                , NULL) ) AS pom_vis_facebook_forms_venta_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                IF(
                (ITE_QUOT_SOURCE IN ('facebook') OR QUESTION_APP_ID IN (6606988212253970,6557539776310564) ) -- FB FORMS
            OR (is_POM_VIS = 'POM VIS'  
              AND POM_VIS_subgroup IN ('FACEBOOK FORMS') )
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , lead_item_id
                , NULL) ) AS pom_vis_facebook_forms_venta_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(
                (ITE_QUOT_SOURCE IN ('facebook') OR QUESTION_APP_ID IN (6606988212253970,6557539776310564) ) -- FB FORMS
            OR (is_POM_VIS = 'POM VIS'  
              AND POM_VIS_subgroup IN ('FACEBOOK FORMS') )
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , B.cus_cust_id_sel
                , NULL) ) AS pom_vis_facebook_forms_venta_vivienda_seller_contacted

        ------------------------------------------------
        ---- POM VIS RTB
        ------------------------------------------------
          -- contact timedecay
          , SAFE_DIVIDE( 
              SUM(IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
              , MATT_PORC_TIMEDECAY
              , NULL) )
            , 100) AS pom_vis_rtb_venta_vivienda_timedecay_contact_attribution

          -- usuarios que contactaron
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , COALESCE(CAST(A.CUS_CUST_ID_BUY AS STRING), B.SESSION_ID)
                , NULL) ) AS pom_vis_rtb_venta_vivienda_user_contact

          -- usuarios logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
              AND A.CUS_CUST_ID_BUY IS NOT NULL
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , A.CUS_CUST_ID_BUY
                , NULL) ) AS pom_vis_rtb_venta_vivienda_user_logg_contact

          -- usuarios no logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
              AND A.CUS_CUST_ID_BUY IS NULL
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , B.SESSION_ID
                , NULL) ) AS pom_vis_rtb_venta_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , lead_item_id
                , NULL) ) AS pom_vis_rtb_venta_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
