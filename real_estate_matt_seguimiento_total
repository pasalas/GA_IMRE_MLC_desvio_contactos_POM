        , SAFE_DIVIDE(pom_vis_brandformance_devs_vivienda_vivienda_user_not_logg_contact, ( COALESCE(pom_vis_brandformance_devs_vivienda_vivienda_user_logg_contact, 0) + COALESCE(pom_vis_brandformance_devs_vivienda_vivienda_user_not_logg_contact,0) ) ) AS perc_pom_vis_brandformance_devs_vivienda_vivienda_user_not_logg_contact
        , pom_vis_brandformance_devs_vivienda_vivienda_item_contacted
        , SAFE_DIVIDE(pom_vis_brandformance_devs_vivienda_vivienda_timedecay_contact_attribution, pom_vis_brandformance_devs_vivienda_vivienda_item_contacted) AS avg_pom_vis_brandformance_devs_vivienda_vivienda_item_contacted
        , pom_vis_brandformance_devs_vivienda_vivienda_seller_contacted
        , SAFE_DIVIDE(pom_vis_brandformance_devs_vivienda_vivienda_timedecay_contact_attribution, pom_vis_brandformance_devs_vivienda_vivienda_seller_contacted) AS avg_pom_vis_brandformance_devs_vivienda_vivienda_seller_contacted


        -- cost clicks impressions
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_devs_vivienda_vivienda_timedecay_contact_attribution) AS pom_vis_brandformance_devs_vivienda_vivienda_lc_cost_per_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_devs_vivienda_vivienda_user_contact) AS pom_vis_brandformance_devs_vivienda_vivienda_lc_cost_per_user_contact
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_devs_vivienda_vivienda_item_contacted) AS pom_vis_brandformance_devs_vivienda_vivienda_lc_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_brandformance_lc_cost, pom_vis_brandformance_devs_vivienda_vivienda_seller_contacted) AS pom_vis_brandformance_devs_vivienda_vivienda_lc_cost_per_seller_contacted

        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_devs_vivienda_vivienda_timedecay_contact_attribution) AS pom_vis_brandformance_devs_vivienda_vivienda_usd_cost_per_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_devs_vivienda_vivienda_user_contact) AS pom_vis_brandformance_devs_vivienda_vivienda_usd_cost_per_user_contact
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_devs_vivienda_vivienda_item_contacted) AS pom_vis_brandformance_devs_vivienda_vivienda_usd_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_devs_vivienda_vivienda_seller_contacted) AS pom_vis_brandformance_devs_vivienda_vivienda_usd_cost_per_seller_contacted





FROM matt_attribution AS A

LEFT JOIN matt_inbound AS B
ON A.sit_site_id = B.sit_site_id
AND A.period = B.period



LEFT JOIN cost_agrupado AS C
ON A.sit_site_id = C.sit_site_id
AND A.period = C.period


ORDER BY 1,2,3
;
