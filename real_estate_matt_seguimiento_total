        , SAFE_DIVIDE(pom_vis_facebook_forms_usd_cost, pom_vis_facebook_forms_vivienda_item_contacted) AS pom_vis_facebook_forms_vivienda_usd_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_facebook_forms_usd_cost, pom_vis_facebook_forms_vivienda_seller_contacted) AS pom_vis_facebook_forms_vivienda_usd_cost_per_seller_contacted

