              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses')
                , B.SESSION_ID
                , NULL) ) AS matt_other_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                IF(
                    `meli-bi-data.SBOX_MKTVIS`.fx_grouped_attribution(C.ite_quot_source,B.mapp_app_id,H.matt_vertical,H.matt_campaign_type,H.matt_implementation_channel,A.matt_type_source,A.matt_stimulus_source_type,A.matt_strategy,A.matt_source,A.matt_channel,H.matt_tool) = 'MATT OTHER'
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses')
                , lead_item_id
                , NULL) ) AS matt_other_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(
                    `meli-bi-data.SBOX_MKTVIS`.fx_grouped_attribution(C.ite_quot_source,B.mapp_app_id,H.matt_vertical,H.matt_campaign_type,H.matt_implementation_channel,A.matt_type_source,A.matt_stimulus_source_type,A.matt_strategy,A.matt_source,A.matt_channel,H.matt_tool) = 'MATT OTHER'
                , B.cus_cust_id_sel
                , NULL) ) AS matt_other_vivienda_seller_contacted

        ------------------------------------------------
        ---- POM VIS desvio
        ------------------------------------------------
          -- contact timedecay
          , SAFE_DIVIDE( 
