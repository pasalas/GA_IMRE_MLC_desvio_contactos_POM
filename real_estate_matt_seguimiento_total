      ------------------------------------------------
      ---- Alquiler vivienda
      ------------------------------------------------
        ------------------------------------------------
        ---- TOTAL (sin apertura de canal)
        ------------------------------------------------
          -- contact timedecay
          , COUNT(DISTINCT IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')  , LEAD_ID, NULL) ) AS total_alquiler_vivienda_contact

          -- usuarios que contactaron
          , COUNT(
              DISTINCT
                IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
              , COALESCE(CAST(A.CUS_CUST_ID_BUY AS STRING), B.SESSION_ID)
              , NULL) 
                ) AS total_alquiler_vivienda_user_contact

          -- usuarios logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                  A.CUS_CUST_ID_BUY IS NOT NULL
                AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , A.CUS_CUST_ID_BUY
                , NULL) ) AS total_alquiler_vivienda_user_logg_contact

          -- usuarios no logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                  A.CUS_CUST_ID_BUY IS NULL
                AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , B.SESSION_ID
                , NULL) ) AS total_alquiler_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                  IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel'), lead_item_id, NULL)
              ) AS total_alquiler_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel'), B.cus_cust_id_sel, NULL)
                ) AS total_alquiler_vivienda_seller_contacted
        ------------------------------------------------
        ---- POM VIS
        ------------------------------------------------
          -- contact timedecay
          , SAFE_DIVIDE( 
              SUM(IF(
                    `meli-bi-data.SBOX_MKTVIS`.fx_grouped_attribution(C.ite_quot_source,B.mapp_app_id,H.matt_vertical,H.matt_campaign_type,H.matt_implementation_channel,A.matt_type_source,A.matt_stimulus_source_type,A.matt_strategy,A.matt_source,A.matt_channel,H.matt_tool) = 'POM VIS'
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
              , MATT_PORC_TIMEDECAY
              , NULL) )
            , 100) AS pom_vis_alquiler_vivienda_timedecay_contact_attribution

          -- usuarios que contactaron
          , COUNT(
              DISTINCT
                IF(
                    `meli-bi-data.SBOX_MKTVIS`.fx_grouped_attribution(C.ite_quot_source,B.mapp_app_id,H.matt_vertical,H.matt_campaign_type,H.matt_implementation_channel,A.matt_type_source,A.matt_stimulus_source_type,A.matt_strategy,A.matt_source,A.matt_channel,H.matt_tool) = 'POM VIS'
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
