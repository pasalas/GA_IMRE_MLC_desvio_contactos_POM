              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND A.CUS_CUST_ID_BUY IS NULL
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Venta','Venda')
                , B.SESSION_ID
                , NULL) ) AS pom_vis_venta_vivienda_user_not_logg_contact

