        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alq_tmp_vivienda_timedecay_contact_attribution) AS pom_vis_brandformance_alq_tmp_vivienda_usd_cost_per_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alq_tmp_vivienda_user_contact) AS pom_vis_brandformance_alq_tmp_vivienda_usd_cost_per_user_contact
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alq_tmp_vivienda_item_contacted) AS pom_vis_brandformance_alq_tmp_vivienda_usd_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_brandformance_usd_cost, pom_vis_brandformance_alq_tmp_vivienda_seller_contacted) AS pom_vis_brandformance_alq_tmp_vivienda_usd_cost_per_seller_contacted


    ------------------------------------------------
    ---- Developments vivienda
    ------------------------------------------------

      ------------------------
      -- TOTAL (sin apertura de canal)
      ------------------------
        -- leads
        , total_devs_vivienda_vivienda_contact
        , SAFE_DIVIDE(pom_vis_devs_vivienda_vivienda_timedecay_contact_attribution, total_devs_vivienda_vivienda_contact) AS perc_pom_vis_devs_vivienda_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(direct_devs_vivienda_vivienda_timedecay_contact_attribution, total_devs_vivienda_vivienda_contact) AS perc_direct_devs_vivienda_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(seo_devs_vivienda_vivienda_timedecay_contact_attribution, total_devs_vivienda_vivienda_contact) AS perc_seo_devs_vivienda_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(own_channels_devs_vivienda_vivienda_timedecay_contact_attribution, total_devs_vivienda_vivienda_contact) AS perc_own_channels_devs_vivienda_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(matt_other_devs_vivienda_vivienda_timedecay_contact_attribution, total_devs_vivienda_vivienda_contact) AS perc_matt_other_devs_vivienda_vivienda_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_desvio_devs_vivienda_vivienda_timedecay_contact_attribution, total_devs_vivienda_vivienda_contact) AS perc_pom_vis_desvio_devs_vivienda_vivienda_timedecay_contact_attribution

        , total_devs_vivienda_vivienda_user_contact
        , SAFE_DIVIDE(pom_vis_devs_vivienda_vivienda_user_contact ,(COALESCE(pom_vis_devs_vivienda_vivienda_user_contact, 0) + COALESCE(direct_devs_vivienda_vivienda_user_contact, 0) + COALESCE(seo_devs_vivienda_vivienda_user_contact, 0) + COALESCE(own_channels_devs_vivienda_vivienda_user_contact, 0) + COALESCE(matt_other_devs_vivienda_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_devs_vivienda_vivienda_user_contact, 0) ) ) perc_pom_vis_devs_vivienda_vivienda_user_contact

        , SAFE_DIVIDE(direct_devs_vivienda_vivienda_user_contact ,(COALESCE(pom_vis_devs_vivienda_vivienda_user_contact, 0) + COALESCE(direct_devs_vivienda_vivienda_user_contact, 0) + COALESCE(seo_devs_vivienda_vivienda_user_contact, 0) + COALESCE(own_channels_devs_vivienda_vivienda_user_contact, 0) + COALESCE(matt_other_devs_vivienda_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_devs_vivienda_vivienda_user_contact, 0) ) ) perc_direct_devs_vivienda_vivienda_user_contact

        , SAFE_DIVIDE(seo_devs_vivienda_vivienda_user_contact ,(COALESCE(pom_vis_devs_vivienda_vivienda_user_contact, 0) + COALESCE(direct_devs_vivienda_vivienda_user_contact, 0) + COALESCE(seo_devs_vivienda_vivienda_user_contact, 0) + COALESCE(own_channels_devs_vivienda_vivienda_user_contact, 0) + COALESCE(matt_other_devs_vivienda_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_devs_vivienda_vivienda_user_contact, 0) ) ) perc_seo_devs_vivienda_vivienda_user_contact

        , SAFE_DIVIDE(own_channels_devs_vivienda_vivienda_user_contact ,(COALESCE(pom_vis_devs_vivienda_vivienda_user_contact, 0) + COALESCE(direct_devs_vivienda_vivienda_user_contact, 0) + COALESCE(seo_devs_vivienda_vivienda_user_contact, 0) + COALESCE(own_channels_devs_vivienda_vivienda_user_contact, 0) + COALESCE(matt_other_devs_vivienda_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_devs_vivienda_vivienda_user_contact, 0) ) ) perc_own_channels_devs_vivienda_vivienda_user_contact

        , SAFE_DIVIDE(matt_other_devs_vivienda_vivienda_user_contact ,(COALESCE(pom_vis_devs_vivienda_vivienda_user_contact, 0) + COALESCE(direct_devs_vivienda_vivienda_user_contact, 0) + COALESCE(seo_devs_vivienda_vivienda_user_contact, 0) + COALESCE(own_channels_devs_vivienda_vivienda_user_contact, 0) + COALESCE(matt_other_devs_vivienda_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_devs_vivienda_vivienda_user_contact, 0) ) ) perc_matt_other_devs_vivienda_vivienda_user_contact

        , SAFE_DIVIDE(pom_vis_desvio_devs_vivienda_vivienda_user_contact ,(COALESCE(pom_vis_devs_vivienda_vivienda_user_contact, 0) + COALESCE(direct_devs_vivienda_vivienda_user_contact, 0) + COALESCE(seo_devs_vivienda_vivienda_user_contact, 0) + COALESCE(own_channels_devs_vivienda_vivienda_user_contact, 0) + COALESCE(matt_other_devs_vivienda_vivienda_user_contact, 0) + COALESCE(pom_vis_desvio_devs_vivienda_vivienda_user_contact, 0) ) ) perc_pom_vis_desvio_devs_vivienda_vivienda_user_contact



        , SAFE_DIVIDE(total_devs_vivienda_vivienda_contact, total_devs_vivienda_vivienda_user_contact) AS avg_devs_vivienda_vivienda_contact_per_user
        , total_devs_vivienda_vivienda_user_logg_contact
        , total_devs_vivienda_vivienda_user_not_logg_contact
        , SAFE_DIVIDE(total_devs_vivienda_vivienda_user_logg_contact, ( COALESCE(total_devs_vivienda_vivienda_user_logg_contact, 0) + COALESCE(total_devs_vivienda_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_devs_vivienda_vivienda_user_logg_contact
        , SAFE_DIVIDE(total_devs_vivienda_vivienda_user_not_logg_contact, ( COALESCE(total_devs_vivienda_vivienda_user_logg_contact, 0) + COALESCE(total_devs_vivienda_vivienda_user_not_logg_contact, 0) ) ) AS perc_total_devs_vivienda_vivienda_user_not_logg_contact
        , total_devs_vivienda_vivienda_item_contacted
        , SAFE_DIVIDE(total_devs_vivienda_vivienda_contact, total_devs_vivienda_vivienda_item_contacted) AS avg_total_devs_vivienda_vivienda_item_contacted
        , total_devs_vivienda_vivienda_seller_contacted
        , SAFE_DIVIDE(total_devs_vivienda_vivienda_contact, total_devs_vivienda_vivienda_seller_contacted) AS avg_total_devs_vivienda_vivienda_seller_contacted


