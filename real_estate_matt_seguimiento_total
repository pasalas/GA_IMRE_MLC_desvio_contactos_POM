              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT')
                , COALESCE(CAST(A.CUS_CUST_ID_BUY AS STRING), B.SESSION_ID)
                , NULL) ) AS pom_vis_brandformance_devs_vivienda_vivienda_user_contact

          -- usuarios logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND A.CUS_CUST_ID_BUY IS NOT NULL
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT')
                , A.CUS_CUST_ID_BUY
                , NULL) ) AS pom_vis_brandformance_devs_vivienda_vivienda_user_logg_contact

          -- usuarios no logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND A.CUS_CUST_ID_BUY IS NULL
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT')
                , B.SESSION_ID
                , NULL) ) AS pom_vis_brandformance_devs_vivienda_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT')
                , lead_item_id
                , NULL) ) AS pom_vis_brandformance_devs_vivienda_vivienda_item_contacted

          -- sellers contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT')
                , B.cus_cust_id_sel
                , NULL) ) AS pom_vis_brandformance_devs_vivienda_vivienda_seller_contacted

              
  FROM `growth-attribution.production.BT_MATT_ATTRIBUTION_LEADS` AS A

  -- traigo tools asociadas a VIS
  LEFT JOIN generic_pom_vis AS POM_GEN
  ON A.PMS_TOOL_ID = POM_GEN.matt_tool

  -- para joinear con fuente de d√≥nde MATT toma leads de questions, call, whatsapp y quotation
  LEFT JOIN `meli-bi-data.WHOWNER.BT_CONTACTS_CLASSIFIEDS` AS B
  ON A.lead_id = B.id

  -- para tener info extra de quotations. FB FORMS = ITE_QUOT_SOURCE IN ('facebook')
  LEFT JOIN `meli-bi-data.WHOWNER.BT_ITE_QUOTATION` AS C
  ON CAST(B.EXTERNAL_ENTITY_ID AS STRING) = CAST(C.ITE_QUOTATION_ID AS STRING)

  -- para tener info extra de questions. FB FORMS = QUESTION_APP_ID IN (6606988212253970,6557539776310564)
  LEFT JOIN `meli-bi-data.WHOWNER.BT_ITEM_QUESTIONS` AS D
  ON CAST(B.EXTERNAL_ENTITY_ID AS STRING) = CAST(D.QUES_ID AS STRING)

  -- para tener arbol de categorias
  LEFT JOIN `meli-bi-data.WHOWNER.AG_LK_CAT_CATEGORIES` AS E
  ON E.CAT_CATEG_ID_L7 = CAST(B.CAT_CATEG_ID_L7 AS INTEGER)
  AND E.SIT_SITE_ID = B.SIT_SITE_ID

  -- para joinear con atributos de anuncio real estate
  LEFT JOIN `meli-bi-data.SBOX_CLASSI_PLANNING.LK_ATRIBUTOS_IMRE` AS F
  ON CAST(A.lead_item_id AS STRING) = CAST(F.ite_item_id AS STRING)
  AND A.sit_site_id = F.sit_site_id

  -- para joinear con datos de cada tool id existente en el matt admin
  LEFT JOIN `meli-bi-data.WHOWNER.BT_MATT_METADATA` AS H
  ON A.pms_tool_id = H.matt_tool
  AND H.matt_business_unit = 'VIS' -- solo tool ids de VIS
  AND H.tim_day = CURRENT_DATE -- para traer el listado actualizado sin duplicar informacion

  WHERE A.LEAD_CREATED_DT BETWEEN fecha_desde AND fecha_hasta
  AND LEAD_ATTRIBUTION_TYPE = 'VIS' -- para tener metricas relacionadas a VIS (en la tabla convive otra BU)
  AND LEAD_TYPE IN ('Quotation','Question','Click to Whatsapp','Click to Call') -- por el momento truncar en estos leads porque los de simulacion de cr√©dito necesitan un cambio que todav√≠a est√° pending por parte de MATT IT
  AND B.cat_categ_id_l1 = parametro_vertical
  AND A.sit_site_id IN UNNEST(var_sites)
  -- AND CAST(POM_GEN.matt_tool AS STRING) IN UNNEST(var_tool_id)



  GROUP BY 1,2,3

)

, cost AS
(
  SELECT
      DATE_TRUNC(A.tim_day, DAY) AS period -- DATE_TRUNC(A.tim_day, MONTH) DATE_TRUNC(A.tim_day, ISOWEEK) 
    , is_POM_VIS
    , POM_VIS_subgroup
    , experimentos
    , A.sit_site_id
    , CLICKS
    , IMPRESSIONS
    , COST_USD
    , COST_LC



  FROM `meli-bi-data.SBOX_MARKETING.BT_COST_GOOGLE_DAILY` AS A

  -- traigo tools asociadas a VIS
  INNER JOIN generic_pom_vis AS POM_GEN
  ON A.tool_id = POM_GEN.matt_tool


  WHERE A.tim_day BETWEEN fecha_desde AND fecha_hasta
  AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)


  UNION ALL
  SELECT
      DATE_TRUNC(A.tim_day, DAY) AS period -- DATE_TRUNC(A.tim_day, MONTH) DATE_TRUNC(A.tim_day, ISOWEEK) 
    , is_POM_VIS
    , POM_VIS_subgroup
    , experimentos
    , A.sit_site_id
    , CLICKS
    , IMPRESSIONS
    , COST_USD
    , COST_LC

  FROM `meli-bi-data.SBOX_MARKETING.BT_COST_FACEBOOK_DAILY` AS A

  -- traigo tools asociadas a VIS
  INNER JOIN generic_pom_vis AS POM_GEN
  ON A.tool_id = POM_GEN.matt_tool


  WHERE A.tim_day BETWEEN fecha_desde AND fecha_hasta
  AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)

  UNION ALL
  SELECT
      DATE_TRUNC(event_date, DAY) AS period -- DATE_TRUNC(event_date, MONTH) DATE_TRUNC(event_date, ISOWEEK) 
    , is_POM_VIS
    , POM_VIS_subgroup
    , experimentos
    , A.sit_site_id
    , CLICKS
    , IMPRESSIONS
    , COST_USD
    , COST_LC


  FROM `meli-bi-data.SBOX_MARKETING.BT_COST_RTB_DAILY` AS A

  -- traigo tools asociadas a VIS
  INNER JOIN generic_pom_vis AS POM_GEN
  ON A.tool_id = POM_GEN.matt_tool

  WHERE A.event_date BETWEEN fecha_desde AND fecha_hasta
  AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)

)

, cost_agrupado AS
(
  SELECT
      sit_site_id
    , period

      ------------------------------------------------
      ---- POM VIS
      ------------------------------------------------
        , SUM(
          IF(
            is_POM_VIS = 'POM VIS'  
          , CLICKS
          , NULL) ) AS pom_vis_clicks

        , SUM(
          IF(
            is_POM_VIS = 'POM VIS'  
          , IMPRESSIONS
          , NULL) ) AS pom_vis_impressions

        , SUM(
          IF(
            is_POM_VIS = 'POM VIS'  
          , COST_USD
          , NULL) ) AS pom_vis_usd_cost

        , SUM(
          IF(
            is_POM_VIS = 'POM VIS'  
          , COST_LC
          , NULL) ) AS pom_vis_lc_cost


      ------------------------------------------------
      ---- POM VIS GOOGLE SEARCH
      ------------------------------------------------
        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE SEARCH')
          , CLICKS
          , NULL) ) AS pom_vis_google_search_clicks

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE SEARCH')
          , IMPRESSIONS
          , NULL) ) AS pom_vis_google_search_impressions

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE SEARCH')
          , COST_USD
          , NULL) ) AS pom_vis_google_search_usd_cost

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE SEARCH')
          , COST_LC
          , NULL) ) AS pom_vis_google_search_lc_cost

      ------------------------------------------------
      ---- POM VIS GOOGLE DISPLAY
      ------------------------------------------------
        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE DISPLAY')
          , CLICKS
          , NULL) ) AS pom_vis_google_display_clicks

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE DISPLAY')
          , IMPRESSIONS
          , NULL) ) AS pom_vis_google_display_impressions

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE DISPLAY')
          , COST_USD
          , NULL) ) AS pom_vis_google_display_usd_cost

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE DISPLAY')
          , COST_LC
          , NULL) ) AS pom_vis_google_display_lc_cost


      ------------------------------------------------
      ---- POM VIS FACEBOOK
      ------------------------------------------------
        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK')
          , CLICKS
          , NULL) ) AS pom_vis_facebook_clicks

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK')
          , IMPRESSIONS
          , NULL) ) AS pom_vis_facebook_impressions

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK')
          , COST_USD
          , NULL) ) AS pom_vis_facebook_usd_cost

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK')
          , COST_LC
          , NULL) ) AS pom_vis_facebook_lc_cost

      ------------------------------------------------
      ---- POM VIS FACEBOOK FORMS
      ------------------------------------------------
        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK FORMS')
          , CLICKS
          , NULL) ) AS pom_vis_facebook_forms_clicks

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK FORMS')
          , IMPRESSIONS
          , NULL) ) AS pom_vis_facebook_forms_impressions

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK FORMS')
          , COST_USD
          , NULL) ) AS pom_vis_facebook_forms_usd_cost

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('FACEBOOK FORMS')
          , COST_LC
          , NULL) ) AS pom_vis_facebook_forms_lc_cost

      ------------------------------------------------
      ---- POM VIS RTB
      ------------------------------------------------
        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
          , CLICKS
          , NULL) ) AS pom_vis_rtb_clicks

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
          , IMPRESSIONS
          , NULL) ) AS pom_vis_rtb_impressions

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
          , COST_USD
          , NULL) ) AS pom_vis_rtb_usd_cost

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('RTB')
          , COST_LC
          , NULL) ) AS pom_vis_rtb_lc_cost

      ------------------------------------------------
      ---- POM VIS OTHER
      ------------------------------------------------
        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('other')
          , CLICKS
          , NULL) ) AS pom_vis_other_clicks

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('other')
          , IMPRESSIONS
          , NULL) ) AS pom_vis_other_impressions

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('other')
          , COST_USD
          , NULL) ) AS pom_vis_other_usd_cost

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('other')
          , COST_LC
          , NULL) ) AS pom_vis_other_lc_cost

      ------------------------------------------------
      ---- EXP: 01. brandformance
      ------------------------------------------------
        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
          , CLICKS
          , NULL) ) AS pom_vis_brandformance_clicks

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
          , IMPRESSIONS
          , NULL) ) AS pom_vis_brandformance_impressions

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
          , COST_USD
          , NULL) ) AS pom_vis_brandformance_usd_cost

        , SUM(
          IF(
                is_POM_VIS = 'POM VIS'  
            AND experimentos IN ('01. brandformance')
          , COST_LC
          , NULL) ) AS pom_vis_brandformance_lc_cost



  FROM cost

  GROUP BY 1,2

)

SELECT -- A.*, B.* EXCEPT(sit_site_id, period), C.* EXCEPT(sit_site_id, period)
    A.vertical
  , A.sit_site_id
  , A.period
    ------------------------
    -- REAL ESTATE
    ------------------------
      ------------------------
      -- TOTAL (sin apertura de canal)
      ------------------------
        -- leads
        , total_realestate_contact
        , SAFE_DIVIDE(pom_vis_realestate_timedecay_contact_attribution, total_realestate_contact) AS perc_pom_vis_realestate_timedecay_contact_attribution
        , SAFE_DIVIDE(direct_realestate_timedecay_contact_attribution, total_realestate_contact) AS perc_direct_realestate_timedecay_contact_attribution
        , SAFE_DIVIDE(seo_realestate_timedecay_contact_attribution, total_realestate_contact) AS perc_seo_realestate_timedecay_contact_attribution
        , SAFE_DIVIDE(own_channels_realestate_timedecay_contact_attribution, total_realestate_contact) AS perc_own_channels_realestate_timedecay_contact_attribution
        , SAFE_DIVIDE(matt_other_realestate_timedecay_contact_attribution, total_realestate_contact) AS perc_matt_other_realestate_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_desvio_realestate_timedecay_contact_attribution, total_realestate_contact) AS perc_pom_vis_desvio_realestate_timedecay_contact_attribution

        , total_realestate_user_contact
        , SAFE_DIVIDE(pom_vis_realestate_user_contact ,(COALESCE(pom_vis_realestate_user_contact, 0) + COALESCE(direct_realestate_user_contact, 0) + COALESCE(seo_realestate_user_contact, 0) + COALESCE(own_channels_realestate_user_contact, 0) + COALESCE(matt_other_realestate_user_contact, 0) + COALESCE(pom_vis_desvio_realestate_user_contact, 0) ) ) perc_pom_vis_realestate_user_contact

        , SAFE_DIVIDE(direct_realestate_user_contact ,(COALESCE(pom_vis_realestate_user_contact, 0) + COALESCE(direct_realestate_user_contact, 0) + COALESCE(seo_realestate_user_contact, 0) + COALESCE(own_channels_realestate_user_contact, 0) + COALESCE(matt_other_realestate_user_contact, 0) + COALESCE(pom_vis_desvio_realestate_user_contact, 0) ) ) perc_direct_realestate_user_contact

        , SAFE_DIVIDE(seo_realestate_user_contact ,(COALESCE(pom_vis_realestate_user_contact, 0) + COALESCE(direct_realestate_user_contact, 0) + COALESCE(seo_realestate_user_contact, 0) + COALESCE(own_channels_realestate_user_contact, 0) + COALESCE(matt_other_realestate_user_contact, 0) + COALESCE(pom_vis_desvio_realestate_user_contact, 0) ) ) perc_seo_realestate_user_contact

        , SAFE_DIVIDE(own_channels_realestate_user_contact ,(COALESCE(pom_vis_realestate_user_contact, 0) + COALESCE(direct_realestate_user_contact, 0) + COALESCE(seo_realestate_user_contact, 0) + COALESCE(own_channels_realestate_user_contact, 0) + COALESCE(matt_other_realestate_user_contact, 0) + COALESCE(pom_vis_desvio_realestate_user_contact, 0) ) ) perc_own_channels_realestate_user_contact

        , SAFE_DIVIDE(matt_other_realestate_user_contact ,(COALESCE(pom_vis_realestate_user_contact, 0) + COALESCE(direct_realestate_user_contact, 0) + COALESCE(seo_realestate_user_contact, 0) + COALESCE(own_channels_realestate_user_contact, 0) + COALESCE(matt_other_realestate_user_contact, 0) + COALESCE(pom_vis_desvio_realestate_user_contact, 0) ) ) perc_matt_other_realestate_user_contact

        , SAFE_DIVIDE(pom_vis_desvio_realestate_user_contact ,(COALESCE(pom_vis_realestate_user_contact, 0) + COALESCE(direct_realestate_user_contact, 0) + COALESCE(seo_realestate_user_contact, 0) + COALESCE(own_channels_realestate_user_contact, 0) + COALESCE(matt_other_realestate_user_contact, 0) + COALESCE(pom_vis_desvio_realestate_user_contact, 0) ) ) perc_pom_vis_desvio_realestate_user_contact



        , SAFE_DIVIDE(total_realestate_contact, total_realestate_user_contact) AS avg_realestate_contact_per_user
        , total_realestate_user_logg_contact
        , total_realestate_user_not_logg_contact
        , SAFE_DIVIDE(total_realestate_user_logg_contact, ( COALESCE(total_realestate_user_logg_contact, 0) + COALESCE(total_realestate_user_not_logg_contact, 0) ) ) AS perc_total_realestate_user_logg_contact
        , SAFE_DIVIDE(total_realestate_user_not_logg_contact, ( COALESCE(total_realestate_user_logg_contact, 0) + COALESCE(total_realestate_user_not_logg_contact, 0) ) ) AS perc_total_realestate_user_not_logg_contact
        , total_realestate_item_contacted
        , SAFE_DIVIDE(total_realestate_contact, total_realestate_item_contacted) AS avg_total_realestate_item_contacted
        , total_realestate_seller_contacted
        , SAFE_DIVIDE(total_realestate_contact, total_realestate_seller_contacted) AS avg_total_realestate_seller_contacted


      ------------------------
      -- POM VIS TOTAL
      ------------------------
        -- leads
        , pom_vis_realestate_timedecay_contact_attribution
        , pom_vis_realestate_user_contact
        , SAFE_DIVIDE(pom_vis_realestate_timedecay_contact_attribution, pom_vis_realestate_user_contact) AS avg_pom_vis_realestate_contact_per_user
        , pom_vis_realestate_user_logg_contact
        , pom_vis_realestate_user_not_logg_contact
        , SAFE_DIVIDE(pom_vis_realestate_user_logg_contact, ( COALESCE(pom_vis_realestate_user_logg_contact, 0) + COALESCE(pom_vis_realestate_user_not_logg_contact,0) ) ) AS perc_pom_vis_realestate_user_logg_contact
        , SAFE_DIVIDE(pom_vis_realestate_user_not_logg_contact, ( COALESCE(pom_vis_realestate_user_logg_contact, 0) + COALESCE(pom_vis_realestate_user_not_logg_contact,0) ) ) AS perc_pom_vis_realestate_user_not_logg_contact
        , pom_vis_realestate_item_contacted
        , SAFE_DIVIDE(pom_vis_realestate_timedecay_contact_attribution, pom_vis_realestate_item_contacted) AS avg_pom_vis_realestate_item_contacted
        , pom_vis_realestate_seller_contacted
        , SAFE_DIVIDE(pom_vis_realestate_timedecay_contact_attribution, pom_vis_realestate_seller_contacted) AS avg_pom_vis_realestate_seller_contacted

        -- touchpoints
        , pom_vis_user_touch_point
        , pom_vis_user_logg_touch_point
        , pom_vis_user_not_logg_touch_point
        , SAFE_DIVIDE(pom_vis_user_logg_touch_point, ( COALESCE(pom_vis_user_logg_touch_point, 0) + COALESCE(pom_vis_user_not_logg_touch_point, 0) ) ) AS perc_pom_vis_user_logg_touch_point
        , SAFE_DIVIDE(pom_vis_user_not_logg_touch_point, ( COALESCE(pom_vis_user_logg_touch_point, 0) + COALESCE(pom_vis_user_not_logg_touch_point, 0) ) ) AS perc_pom_vis_user_not_logg_touch_point

        -- cost clicks impressions
        , pom_vis_clicks
        , pom_vis_impressions
        , pom_vis_usd_cost
        , pom_vis_lc_cost
        , SAFE_DIVIDE(pom_vis_lc_cost, pom_vis_clicks) AS pom_vis_lc_cost_per_clicks
        , SAFE_DIVIDE(pom_vis_lc_cost, pom_vis_impressions) AS pom_vis_lc_cost_per_impressions
        , SAFE_DIVIDE(pom_vis_lc_cost, pom_vis_user_touch_point) AS pom_vis_lc_cost_per_user_touch_point
        , SAFE_DIVIDE(pom_vis_lc_cost, pom_vis_realestate_timedecay_contact_attribution) AS pom_vis_lc_cost_per_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_lc_cost, pom_vis_realestate_user_contact) AS pom_vis_lc_cost_per_user_contact
        , SAFE_DIVIDE(pom_vis_lc_cost, pom_vis_realestate_item_contacted) AS pom_vis_lc_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_lc_cost, pom_vis_realestate_seller_contacted) AS pom_vis_lc_cost_per_seller_contacted

        , SAFE_DIVIDE(pom_vis_usd_cost, pom_vis_clicks) AS pom_vis_usd_cost_per_clicks
        , SAFE_DIVIDE(pom_vis_usd_cost, pom_vis_impressions) AS pom_vis_usd_cost_per_impressions
        , SAFE_DIVIDE(pom_vis_usd_cost, pom_vis_user_touch_point) AS pom_vis_usd_cost_per_user_touch_point
        , SAFE_DIVIDE(pom_vis_usd_cost, pom_vis_realestate_timedecay_contact_attribution) AS pom_vis_usd_cost_per_timedecay_contact_attribution
        , SAFE_DIVIDE(pom_vis_usd_cost, pom_vis_realestate_user_contact) AS pom_vis_usd_cost_per_user_contact
        , SAFE_DIVIDE(pom_vis_usd_cost, pom_vis_realestate_item_contacted) AS pom_vis_usd_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_usd_cost, pom_vis_realestate_seller_contacted) AS pom_vis_usd_cost_per_seller_contacted


      ------------------------
      -- DIRECT
      ------------------------
        -- leads
        , direct_realestate_timedecay_contact_attribution
        , direct_realestate_user_contact
        , SAFE_DIVIDE(direct_realestate_timedecay_contact_attribution, direct_realestate_user_contact) AS avg_direct_realestate_contact_per_user
        , direct_realestate_user_logg_contact
        , direct_realestate_user_not_logg_contact
        , SAFE_DIVIDE(direct_realestate_user_logg_contact, ( COALESCE(direct_realestate_user_logg_contact, 0) + COALESCE(direct_realestate_user_not_logg_contact,0) ) ) AS perc_direct_realestate_user_logg_contact
        , SAFE_DIVIDE(direct_realestate_user_not_logg_contact, ( COALESCE(direct_realestate_user_logg_contact, 0) + COALESCE(direct_realestate_user_not_logg_contact,0) ) ) AS perc_direct_realestate_user_not_logg_contact
        , direct_realestate_item_contacted
        , SAFE_DIVIDE(direct_realestate_timedecay_contact_attribution, direct_realestate_item_contacted) AS avg_direct_realestate_item_contacted
        , direct_realestate_seller_contacted
        , SAFE_DIVIDE(direct_realestate_timedecay_contact_attribution, direct_realestate_seller_contacted) AS avg_direct_realestate_seller_contacted


