        , SAFE_DIVIDE(pom_vis_rtb_lc_cost, pom_vis_rtb_vivienda_item_contacted) AS pom_vis_rtb_vivienda_lc_cost_per_item_contacted
        , SAFE_DIVIDE(pom_vis_rtb_lc_cost, pom_vis_rtb_vivienda_seller_contacted) AS pom_vis_rtb_vivienda_lc_cost_per_seller_contacted

        , SAFE_DIVIDE(pom_vis_rtb_usd_cost, pom_vis_rtb_vivienda_timedecay_contact_attribution) AS pom_vis_rtb_vivienda_usd_cost_per_timedecay_contact_attribution
