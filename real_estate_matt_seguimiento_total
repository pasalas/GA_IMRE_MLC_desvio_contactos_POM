            AND POM_VIS_subgroup IN ('GOOGLE SEARCH')
              AND A.CUS_CUST_ID_BUY IS NOT NULL
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , A.CUS_CUST_ID_BUY
                , NULL) ) AS pom_vis_google_search_alquiler_vivienda_user_logg_contact

          -- usuarios no logueados que contactaron
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE SEARCH')
              AND A.CUS_CUST_ID_BUY IS NULL
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , B.SESSION_ID
                , NULL) ) AS pom_vis_google_search_alquiler_vivienda_user_not_logg_contact

          -- items contactados
          , COUNT(
              DISTINCT
                IF(
                is_POM_VIS = 'POM VIS'  
            AND POM_VIS_subgroup IN ('GOOGLE SEARCH')
              AND POM_GEN.matt_vertical IN UNNEST(var_matt_vertical)
              AND categoria_l2 IN ('Apartamentos','Casas','Departamentos','PH','Townhouses') AND REGEXP_CONTAINS(ITE_ITEM_DOM_DOMAIN_ID, 'DEVELOPMENT') IS FALSE AND OPERATION IN ('Alquiler','Arriendo','Renta','Aluguel')
                , lead_item_id
                , NULL) ) AS pom_vis_google_search_alquiler_vivienda_item_contacted

